{
  "name": "my_vscode_snippets",
  "author": "ryoppippi",
  "engines": {
    "vscode": "^2023.0.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "c",
        "path": "./snippets/c.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/cpp.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.toml"
      },
      {
        "language": "toml",
        "path": "./snippets/toml.toml"
      },
      {
        "language": [
          "typescript",
          "typescriptreact",
          "svelte",
          "astro",
          "typescript.tsx",
          "vue"
        ],
        "path": "./snippets/typescript.toml"
      },
      {
        "language": "svelte",
        "path": "./snippets/svelte.toml"
      },
      {
        "language": [
          "jsx",
          "tsx",
          "javascriptreact",
          "typescriptreact"
        ],
        "path": "./snippets/jsx.toml"
      }
    ]
  }
}
