{
  "name": "my_vscode_snippets",
  "author": "ryoppippi",
  "engines": {
    "vscode": "^2023.0.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "c",
        "path": "./snippets/c.json"
      },
      {
        "language": "cpp",
        "path": "./snippets/cpp.json"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.toml"
      },
      {
        "language": "toml",
        "path": "./snippets/toml.toml"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.toml"
      }
    ]
  }
}
